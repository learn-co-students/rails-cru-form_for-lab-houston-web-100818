<h1><%= @artist.name %></h1>
<p>Bio: <%= @artist.bio %></p>
<%= link_to "Edit Artist", edit_artist_path %><br>
<br>
<label>Songs:</label>
<ul>
<% @artist.songs.each do |song| %>
    <li><%= link_to song.name, song %></li>
<% end %>
</ul>
<label>Genres:</label>
<ul>
<% @artist.genres.each do |genre| %>
    <li><%= link_to genre.name, genre %></li>
<% end %>
</ul>
